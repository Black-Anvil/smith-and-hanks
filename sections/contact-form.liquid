<section data-component="contact-form" data-component-type="section" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="contact-form">
  <section-wrapper width="content">
    <section-content>
      <div class="contact-form__content">
        {% if section.settings.contact_form_heading != blank %}
          <h2 class="contact-form__heading">
            {{ section.settings.contact_form_heading | escape }}
          </h2>
        {% endif %}
        {% comment %}UUID contact_id begin{% endcomment %}
        {% assign contact_id = '64ed8f58-51c9-47ae-ac02-2b63ad448489' %}
        {% comment %}UUID contact_id end{% endcomment %}
        {% assign contact_id = contact_id | append: '-' | append: section.id %}
        {% form 'contact', id: contact_id, class: 'contact-form__form' %}
          {% if form.posted_successfully? %}
            {% capture form_success_content %}
              <div
                class="contact-form__notification"
                tabindex="-1"
                autofocus
              >
                {{ 'sections.contact_form.post_success' | t }}
              </div>
            {% endcapture %}
            {% comment %}Component notification:default:1.0.0-pre.10 begin{% endcomment %}
            {% liquid
              assign content_97e78f83 = content
              assign content = form_success_content
              assign type_97e78f83 = type
              assign type = 'success'
            %}
            {%- comment -%}
              @param content [Required]
                The content that goes in the notification banner
            
              @param type [Required]
                The type of notification banner ('error' or 'success')
            
              @param hidden [Optional]
                Whether the notification should be hidden
            {%- endcomment -%}
            
            <div
              data-component="notification"
              data-component-type="component"
              {% if tag != blank %}data-component-tag="{{ tag }}"
              {% endif %}class="
                notification
                notification--{{ type }}
              "
              {% if type == 'error' %}
                role="alert"
              {% else %}
                role="status"
              {% endif %}
              {% if hidden %}hidden{% endif %}
            >
              {{ content }}
            </div>
            
            {% liquid
              assign content = content_97e78f83
              assign content_97e78f83 = nil
              assign type = type_97e78f83
              assign type_97e78f83 = nil
            %}
            {% comment %}Component notification:default:1.0.0-pre.10 end{% endcomment %}
          {% elsif form.errors %}
          {% capture form_error_content %}
            <h2 class="contact-form__notification" tabindex="-1" autofocus>{{ 'sections.contact_form.error_heading' | t }}</h2>
            <ul class="contact-form__error-list" role="list">
              <li>
                <a href="#contact-form-email">
                  {{ form.errors.translated_fields['email'] | capitalize }} {{ form.errors.messages['email'] }}
                </a>
              </li>
            </ul>
          {% endcapture %}
          {% comment %}Component notification:default:1.0.0-pre.10 begin{% endcomment %}
          {% liquid
            assign content_d965d2dc = content
            assign content = form_error_content
            assign type_d965d2dc = type
            assign type = 'error'
          %}
          {%- comment -%}
            @param content [Required]
              The content that goes in the notification banner
          
            @param type [Required]
              The type of notification banner ('error' or 'success')
          
            @param hidden [Optional]
              Whether the notification should be hidden
          {%- endcomment -%}
          
          <div
            data-component="notification"
            data-component-type="component"
            {% if tag != blank %}data-component-tag="{{ tag }}"
            {% endif %}class="
              notification
              notification--{{ type }}
            "
            {% if type == 'error' %}
              role="alert"
            {% else %}
              role="status"
            {% endif %}
            {% if hidden %}hidden{% endif %}
          >
            {{ content }}
          </div>
          
          {% liquid
            assign content = content_d965d2dc
            assign content_d965d2dc = nil
            assign type = type_d965d2dc
            assign type_d965d2dc = nil
          %}
          {% comment %}Component notification:default:1.0.0-pre.10 end{% endcomment %}
          {% endif %}

          {% liquid
            assign name_label = 'sections.contact_form.name' | t
            assign email_label = 'sections.contact_form.email' | t
            assign phone_label = 'sections.contact_form.phone' | t
            assign comment_label = 'sections.contact_form.comment' | t
            assign button_label = 'sections.contact_form.send' | t

            assign name_placeholder = 'sections.contact_form.name_placeholder' | t
            assign email_placeholder = 'sections.contact_form.email_placeholder' | t
            assign phone_placeholder = 'sections.contact_form.phone_placeholder' | t
            assign comment_placeholder = 'sections.contact_form.comment_placeholder' | t
          %}

          {%- capture name_label_text -%}
            {{ name_label }} <span aria-hidden="true">*</span>
          {%- endcapture -%}

          {%- capture email_label_text -%}
            {{ email_label }} <span aria-hidden="true">*</span>
          {%- endcapture -%}

          {%- capture comment_label_text -%}
            {{ comment_label }} <span aria-hidden="true">*</span>
          {%- endcapture -%}

          {% capture name_attributes %}
            aria-required="true"
            autocomplete="name"
            required
          {% endcapture %}

          {% capture phone_attributes %}
            autocomplete="tel"
            pattern="[0-9\-]*"
          {% endcapture %}

          {% capture email_attributes %}
            aria-required="true"
            required
            {% if form.errors contains 'email' %}
              aria-describedby="contact-form-email-error"
            {% endif %}
          {% endcapture %}

          {% capture comment_attributes %}
            aria-required="true"
            required
          {% endcapture %}
          <div class="contact-form__field-wrapper">
            {% assign input_id = contact_id | append: '-name' %}
            {% comment %}Component input-field:default:1.0.0-pre.10 begin{% endcomment %}
            {% liquid
              assign input_type_749a4c96 = input_type
              assign input_type = 'text'
              assign label_text_749a4c96 = label_text
              assign label_text = name_label_text
              assign form_namespace_749a4c96 = form_namespace
              assign form_namespace = 'contact'
              assign field_name_749a4c96 = field_name
              assign field_name = 'name'
              assign placeholder_749a4c96 = placeholder
              assign placeholder = name_placeholder
              assign attributes_749a4c96 = attributes
              assign attributes = name_attributes
            %}
            {% comment %}
              @param form [Required]
                The form that the input lives in.
            
              @param form_namespace [Required]
                The form's namespace that is used in "name" attribute as form_namespace[field_name].
            
              @param field_name [Required]
                The field name for the form. Will be used for the "name" attribute.
            
              @param input_type [Required]
                The type of field that is being rendered. Usually text, password, number, etc.
            
              @param input_id [Optional]
                The type of field that is being rendered. Usually text, password, number, etc.
            
              @param label_text [Required]
                The text for the label to be included.
            
              @param placeholder [Optional]
                The placeholder for the input field.
            
              @param attributes [Optional]
                Additional attributes to append to the input field.
            {% endcomment %}
            
            {% if input_id == blank %}
              {% comment %}UUID input_id begin{% endcomment %}
              {% assign input_id = '0cb3a06d-5aef-4606-bbd8-efee2be8da95' %}
              {% comment %}UUID input_id end{% endcomment %}
            {% endif %}
            
            <div data-component="input-field" data-component-type="component" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="input-field">
              <label for="{{ input_id }}">{{ label_text }}</label>
              <input
                id="{{ input_id }}"
                type="{{ input_type }}"
                name="{% if form_namespace %}{{ form_namespace }}[{{ field_name }}]{% else %}{{ field_name }}{% endif %}"
                value="{{ form[field_name] }}"
                placeholder="{{ placeholder }}"
                {{ attributes }}
              >
            </div>
            
            {% liquid
              assign input_type = input_type_749a4c96
              assign input_type_749a4c96 = nil
              assign label_text = label_text_749a4c96
              assign label_text_749a4c96 = nil
              assign form_namespace = form_namespace_749a4c96
              assign form_namespace_749a4c96 = nil
              assign field_name = field_name_749a4c96
              assign field_name_749a4c96 = nil
              assign placeholder = placeholder_749a4c96
              assign placeholder_749a4c96 = nil
              assign attributes = attributes_749a4c96
              assign attributes_749a4c96 = nil
            %}
            {% comment %}Component input-field:default:1.0.0-pre.10 end{% endcomment %}
            {% assign input_id = contact_id | append: '-email' %}
            {% comment %}Component input-email:default:1.0.0-pre.10 begin{% endcomment %}
            {% liquid
              assign label_text_5e52499f = label_text
              assign label_text = email_label_text
              assign form_namespace_5e52499f = form_namespace
              assign form_namespace = 'contact'
              assign field_name_5e52499f = field_name
              assign field_name = 'email'
              assign placeholder_5e52499f = placeholder
              assign placeholder = email_placeholder
              assign attributes_5e52499f = attributes
              assign attributes = email_attributes
            %}
            {% comment %}
              @param form [Required]
                The form that the input lives in.
            
              @param label_text [Required]
                The text for the label to be included.
            
              @param form_namespace [Required]
                The form's namespace that is used in "name" attribute as form_namespace[field_name].
            
              @param field_name [Required]
                The field name for the form. Will be used for the "name" attribute.
            
              @param input_id [Optional]
                The type of field that is being rendered. Usually text, password, number, etc.
            
              @param placeholder [Optional]
                The placeholder for the input field.
            
              @param attributes [Optional]
                Additional attributes to append to the input field.
            {% endcomment %}
            
            {% if input_id == blank %}
              {% comment %}UUID input_id begin{% endcomment %}
              {% assign input_id = '59db0618-5d5f-4d46-a95c-da251c6d4999' %}
              {% comment %}UUID input_id end{% endcomment %}
            {% endif %}
            
            <div data-component="input-email" data-component-type="component" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="input-field">
              <label for="{{ input_id }}">{{ label_text }}</label>
              <input
                id="{{ input_id }}"
                type="email"
                name="{% if form_namespace %}{{ form_namespace }}[{{ field_name }}]{% else %}{{ field_name }}{% endif %}"
                value="{{ form[field_name] }}"
                placeholder="{{ placeholder }}"
                autocapitalize="off"
                autocorrect="off"
                spellcheck="false"
                {% if form.errors contains 'email' %}
                  aria-invalid="true"
                {% endif %}
                {{ attributes }}
              >
            </div>
            
            {% liquid
              assign label_text = label_text_5e52499f
              assign label_text_5e52499f = nil
              assign form_namespace = form_namespace_5e52499f
              assign form_namespace_5e52499f = nil
              assign field_name = field_name_5e52499f
              assign field_name_5e52499f = nil
              assign placeholder = placeholder_5e52499f
              assign placeholder_5e52499f = nil
              assign attributes = attributes_5e52499f
              assign attributes_5e52499f = nil
            %}
            {% comment %}Component input-email:default:1.0.0-pre.10 end{% endcomment %}
          </div>
          {% assign input_id = contact_id | append: '-phone-number' %}
          {% comment %}Component input-field:default:1.0.0-pre.10 begin{% endcomment %}
          {% liquid
            assign input_type_860a89c4 = input_type
            assign input_type = 'tel'
            assign label_text_860a89c4 = label_text
            assign label_text = phone_label
            assign form_namespace_860a89c4 = form_namespace
            assign form_namespace = 'contact'
            assign field_name_860a89c4 = field_name
            assign field_name = 'Phone number'
            assign placeholder_860a89c4 = placeholder
            assign placeholder = phone_placeholder
            assign attributes_860a89c4 = attributes
            assign attributes = phone_attributes
          %}
          {% comment %}
            @param form [Required]
              The form that the input lives in.
          
            @param form_namespace [Required]
              The form's namespace that is used in "name" attribute as form_namespace[field_name].
          
            @param field_name [Required]
              The field name for the form. Will be used for the "name" attribute.
          
            @param input_type [Required]
              The type of field that is being rendered. Usually text, password, number, etc.
          
            @param input_id [Optional]
              The type of field that is being rendered. Usually text, password, number, etc.
          
            @param label_text [Required]
              The text for the label to be included.
          
            @param placeholder [Optional]
              The placeholder for the input field.
          
            @param attributes [Optional]
              Additional attributes to append to the input field.
          {% endcomment %}
          
          {% if input_id == blank %}
            {% comment %}UUID input_id begin{% endcomment %}
            {% assign input_id = '6b2e0b75-b283-4b35-b9cb-ce3f8326dbf8' %}
            {% comment %}UUID input_id end{% endcomment %}
          {% endif %}
          
          <div data-component="input-field" data-component-type="component" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="input-field">
            <label for="{{ input_id }}">{{ label_text }}</label>
            <input
              id="{{ input_id }}"
              type="{{ input_type }}"
              name="{% if form_namespace %}{{ form_namespace }}[{{ field_name }}]{% else %}{{ field_name }}{% endif %}"
              value="{{ form[field_name] }}"
              placeholder="{{ placeholder }}"
              {{ attributes }}
            >
          </div>
          
          {% liquid
            assign input_type = input_type_860a89c4
            assign input_type_860a89c4 = nil
            assign label_text = label_text_860a89c4
            assign label_text_860a89c4 = nil
            assign form_namespace = form_namespace_860a89c4
            assign form_namespace_860a89c4 = nil
            assign field_name = field_name_860a89c4
            assign field_name_860a89c4 = nil
            assign placeholder = placeholder_860a89c4
            assign placeholder_860a89c4 = nil
            assign attributes = attributes_860a89c4
            assign attributes_860a89c4 = nil
          %}
          {% comment %}Component input-field:default:1.0.0-pre.10 end{% endcomment %}
          {% assign input_id = contact_id | append: '-comment' %}
          {% comment %}Component input-textarea:default:1.0.0-pre.10 begin{% endcomment %}
          {% liquid
            assign label_text_0c56eb45 = label_text
            assign label_text = comment_label_text
            assign form_namespace_0c56eb45 = form_namespace
            assign form_namespace = 'contact'
            assign field_name_0c56eb45 = field_name
            assign field_name = 'comment'
            assign form_id_0c56eb45 = form_id
            assign form_id = form.id
            assign input_text_0c56eb45 = input_text
            assign input_text = form.body
            assign placeholder_0c56eb45 = placeholder
            assign placeholder = comment_placeholder
            assign attributes_0c56eb45 = attributes
            assign attributes = comment_attributes
            assign rows_0c56eb45 = rows
            assign rows = 5
          %}
          {% comment %}
            @param label_text [Required]
              The text for the label to be included.
          
            @param form_namespace [Required]
              The form's namespace that is used in "name" attribute as form_namespace[field_name].
          
            @param field_name [Required]
              The field name for the form. Will be used for the "name" attribute.
          
            @param form_id [Optional]
              The form that the input lives in.
          
            @param input_id [Optional]
              The type of field that is being rendered. Usually text, password, number, etc.
          
            @param input_text [Optional]
              The initial text to display within the textarea
          
            @param placeholder [Optional]
              The placeholder for the input field.
          
            @param attributes [Optional]
              Additional attributes to append to the input field.
          
            @param rows [Optional]
              The number of rows for the textarea (height)
          
            @param columns [Optional]
              The number of columns for the textarea (width)
          {% endcomment %}
          
          {% if input_id == blank %}
            {% comment %}UUID input_id begin{% endcomment %}
            {% assign input_id = '13113a38-cf78-4bd2-8133-b08517359fe6' %}
            {% comment %}UUID input_id end{% endcomment %}
          {% endif %}
          
          <div data-component="input-textarea" data-component-type="component" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="textarea">
            <label for="{{ input_id }}">{{ label_text }}</label>
            <textarea
              id="{{ input_id }}"
              name="{% if form_namespace %}{{ form_namespace }}[{{ field_name }}]{% else %}{{ field_name }}{% endif %}"
              {% if form_id %}
                form="{{ form_id }}"
              {% endif %}
              {% if rows %}rows="{{ rows }}"{% endif %}
              {% if columns %}cols="{{ columns }}"{% endif %}
              placeholder="{{ placeholder }}"
              {{ attributes }}
              data-input
            >
              {%- if input_text != blank -%}{{- input_text -}}{%- endif -%}
            </textarea>
          </div>
          
          {% liquid
            assign label_text = label_text_0c56eb45
            assign label_text_0c56eb45 = nil
            assign form_namespace = form_namespace_0c56eb45
            assign form_namespace_0c56eb45 = nil
            assign field_name = field_name_0c56eb45
            assign field_name_0c56eb45 = nil
            assign form_id = form_id_0c56eb45
            assign form_id_0c56eb45 = nil
            assign input_text = input_text_0c56eb45
            assign input_text_0c56eb45 = nil
            assign placeholder = placeholder_0c56eb45
            assign placeholder_0c56eb45 = nil
            assign attributes = attributes_0c56eb45
            assign attributes_0c56eb45 = nil
            assign rows = rows_0c56eb45
            assign rows_0c56eb45 = nil
          %}
          {% comment %}Component input-textarea:default:1.0.0-pre.10 end{% endcomment %}
          {% comment %}Component button:default:1.0.0-pre.10 begin{% endcomment %}
          {% liquid
            assign style_fb5088d3 = style
            assign style = 'primary'
            assign size_fb5088d3 = size
            assign size = 'medium'
            assign text_fb5088d3 = text
            assign text = button_label
          %}
          {% comment %}
            @param style [Required]
              This is the style of the button: primary, secondary, tertiary, or text. Default primary.
          
            @param size [Required]
              This defines the size of the button: small, medium, or large. Default medium.
          
            @param text [Required]
              This is the text to be included in the button.
          
            @param attributes [Optional]
              This will include any additional attributes that the button needs. For example, aria-controls.
          {% endcomment %}
          
          <button
            data-component="button"
            data-component-type="component"
            {% if tag != blank %}data-component-tag="{{ tag }}"
            {% endif %}class="button button--{{ style }} button--{{ size }}{% if style == 'primary' or style == 'secondary' %} button--shadow{% endif %%}"
            {{ attributes }}
          >
            {{ text }}
          </button>
          
          {% liquid
            assign style = style_fb5088d3
            assign style_fb5088d3 = nil
            assign size = size_fb5088d3
            assign size_fb5088d3 = nil
            assign text = text_fb5088d3
            assign text_fb5088d3 = nil
          %}
          {% comment %}Component button:default:1.0.0-pre.10 end{% endcomment %}
        {% endform %}
      </div>
    </section-content>
  </section-wrapper>
</section>
{% schema %}
{
  "name": "Contact form",
  "settings": [
    {
      "type": "text",
      "id": "contact_form_heading",
      "default": "Contact form",
      "label": "Heading"
    }
  ],
  "presets": [
    {
      "name": "Contact form",
      "settings": {
        "contact_form_heading": "Contact form"
      }
    }
  ]
}
{% endschema %}