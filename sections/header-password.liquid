                  {% comment %}Component disclosure:default:1.0.0-pre.10 end{% endcomment %}
                {%- endform -%}
              {% endif %}
            </div>
            
            {% comment %}Component cross-border:default:1.0.0-pre.10 end{% endcomment %}
          </div>
        </div>
      </div>
      
      {% comment %}Component header-toolbar:default:1.0.0-pre.10 end{% endcomment %}
    {% endif %}

    <header
      class="header-password__header"
      data-header-password
      {{ section_attributes }}
    >

      <div
        class="
          header-password__wrapper
          {{ wrapper_classes }}
        "
        data-header-password-wrapper
      >

        <div
          class="header-password__heading"
          style="
            --logo-width: {{ logo_width }}px;
            --logo-width-mobile: {{ logo_width_mobile }}px;
          "
          >
          {% if template contains 'index' %}
            <h1 class="header-password__title">
          {% else %}
            <h2 class="header-password__title">
          {% endif %}
            <a
              href="{{ routes.root_url }}"
              class="
                header-password__logo
                {% if logo %}header-password__logo--has-image{% endif %}
                {% if logo_mobile %}header-password__logo--has-mobile-image{% endif %}
              "
              aria-label="{{ shop.name }}"
              data-header-password-logo
            >
              {% if logo %}
                <div class="header-password__logo-image" data-logo-image>
                  {% comment %}Component lazy-img:default:1.0.0-pre.10 begin{% endcomment %}
                  {% render 'lazy-img', img: logo, size: logo_width_size, lazy: true, canvas: true %}
                  {% comment %}Component lazy-img:default:1.0.0-pre.10 end{% endcomment %}
                </div>
              {% endif %}

              {% if logo_mobile %}
                <div class="header-password__mobile-image" data-logo-image-mobile>
                  {% comment %}Component lazy-img:default:1.0.0-pre.10 begin{% endcomment %}
                  {% render 'lazy-img', img: logo_mobile, size: logo_width_mobile_size, lazy: true, canvas: true %}
                  {% comment %}Component lazy-img:default:1.0.0-pre.10 end{% endcomment %}
                </div>
              {% endif %}

              <span class="header-password__site-name">{{ shop.name }}</span>
            </a>
          {% if template contains 'index' %}
            </h1>
          {% else %}
            </h2>
          {% endif %}
        </div>

        <div
          class="header-password__controls header-password__controls--first"
          data-header-password-controls
        >
        </div>

        <div
          class="header-password__controls header-password__controls--last"
          data-header-password-controls
        >
          {% assign text = 'navigation.header_password.login_password_button' | t %}
          {% comment %}Component button:default:1.0.0-pre.10 begin{% endcomment %}
          {% liquid
            assign tag_afb6715d = tag
            assign tag = 'login'
            assign style_afb6715d = style
            assign style = 'text'
            assign size_afb6715d = size
            assign size = 'small'
            assign attributes_afb6715d = attributes
            assign attributes = 'type="button" aria-haspopup="dialog"'
          %}
          {% comment %}
            @param style [Required]
              This is the style of the button: primary, secondary, tertiary, or text. Default primary.
          
            @param size [Required]
              This defines the size of the button: small, medium, or large. Default medium.
          
            @param text [Required]
              This is the text to be included in the button.
          
            @param attributes [Optional]
              This will include any additional attributes that the button needs. For example, aria-controls.
          {% endcomment %}
          
          <button
            data-component="button"
            data-component-type="component"
            {% if tag != blank %}data-component-tag="{{ tag }}"
            {% endif %}class="button button--{{ style }} button--{{ size }}{% if style == 'primary' or style == 'secondary' %} button--shadow{% endif %%}"
            {{ attributes }}
          >
            {{ text }}
          </button>
          
          {% liquid
            assign tag = tag_afb6715d
            assign tag_afb6715d = nil
            assign style = style_afb6715d
            assign style_afb6715d = nil
            assign size = size_afb6715d
            assign size_afb6715d = nil
            assign attributes = attributes_afb6715d
            assign attributes_afb6715d = nil
          %}
          {% comment %}Component button:default:1.0.0-pre.10 end{% endcomment %}
        </div>
      </div>
    </header>

    <div data-header-password-scroll-observer></div>
  </section-wrapper>
</section>
{% schema %}
{
  "name": "Header",
  "class": "section-header-password",
  "settings": [
    {
      "type": "header",
      "content": "Logo"
    },
    {
      "type": "image_picker",
      "id": "header_logo",
      "label": "Logo image",
      "info": "300 x 90px .png recommended."
    },
    {
      "type": "image_picker",
      "id": "mobile_header_logo",
      "label": "Mobile logo image",
      "info": "200 x 90px .png recommended."
    },
    {
      "type": "range",
      "id": "logo_width",
      "label": "Logo max width",
      "step": 5,
      "min": 50,
      "max": 300,
      "default": 200,
      "unit": "px"
    },
    {
      "type": "range",
      "id": "logo_width_mobile",
      "label": "Mobile logo max width",
      "step": 5,
      "min": 50,
      "max": 200,
      "default": 100,
      "unit": "px"
    },
    {
      "type": "checkbox",
      "id": "header_sticky",
      "label": "Enable sticky header",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "show_border",
      "label": "Show separator line",
      "default": true
    },
    {
      "type": "header",
      "content": "Toolbar"
    },
    {
      "type": "checkbox",
      "id": "show_social_icons",
      "label": "Show social icons",
      "default": true
    },
    {
      "type": "color",
      "id": "toolbar_text_color",
      "label": "Text",
      "default": "#444444"
    },
    {
      "type": "color",
      "id": "toolbar_bg_color",
      "label": "Background",
      "default": "#F0F0F0"
    },
    {
      "type": "header",
      "content": "Language selector",
      "info": "To add a language, go to your [language settings](/admin/settings/languages)."
    },
    {
      "type": "checkbox",
      "id": "enable_language_selector",
      "default": true,
      "label": "Show language selector"
    },
    {
      "type": "header",
      "content": "Country/region selector",
      "info": "To add a country/region, go to your [payment settings](/admin/settings/payments)."
    },
    {
      "type": "checkbox",
      "id": "enable_country_selector",
      "default": true,
      "label": "Show country/region selector"
    }
  ],
  "blocks": [
    {
      "type": "announcement",
      "name": "Announcement",
      "settings": [
        {
          "type": "richtext",
          "id": "text",
          "label": "Text",
          "default": "<p>Call attention to a special discount, feature, or promotion here.</p>"
        },
        {
          "type": "url",
          "id": "url",
          "label": "Link",
          "info": "Applies a click-through link on the entire text. If there are links inside the text, this will not be applied."
        }
      ]
    }
  ]
}
{% endschema %}