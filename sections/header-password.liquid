                {% if settings.social_vimeo_link != '' %}
                  <li class="social-media-links__item">
                    <a
                      class="social-media-links__link"
                      href="{{ settings.social_vimeo_link }}"
                      aria-label="{{ shop.name }} on Vimeo"
                      rel="me"
                      target="_blank"
                    >
                      {% comment %}Icon vimeo:default:1.0.0-pre.0 begin{% endcomment %}
                      <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" clip-rule="evenodd" d="M15.994 4.236c-.08 1.568-1.167 3.717-3.262 6.465C10.556 13.56 8.716 15 7.197 15c-.927 0-1.71-.873-2.35-2.618L3.55 7.598C3.07 5.852 2.559 4.98 2.015 4.98c-.128 0-.544.258-1.263.76L0 4.77l2.335-2.102c1.056-.937 1.84-1.422 2.367-1.454 1.248-.146 2.016.71 2.304 2.553.32 1.988.527 3.233.64 3.718.367 1.648.751 2.472 1.183 2.472.336 0 .848-.533 1.52-1.616a6.368 6.368 0 0 0 1.071-2.424c.096-.937-.272-1.406-1.072-1.406a2.93 2.93 0 0 0-1.183.275c.783-2.603 2.287-3.863 4.494-3.782 1.648.048 2.415 1.131 2.335 3.232Z" fill="currentColor"/></svg>
                      {% comment %}Icon vimeo:default:1.0.0-pre.0 end{% endcomment %}
                    </a>
                  </li>
                {% endif %}
              
                {% if settings.social_tiktok_link != '' %}
                  <li class="social-media-links__item">
                    <a
                      class="social-media-links__link"
                      href="{{ settings.social_tiktok_link }}"
                      aria-label="{{ shop.name }} on TikTok"
                      rel="me"
                      target="_blank"
                    >
                      {% comment %}Icon tiktok:default:1.0.0-pre.0 begin{% endcomment %}
                      <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M8.423 0h2.738s-.156 3.51 3.796 3.768v2.711s-2.114.129-3.796-1.158l.028 5.606a5.073 5.073 0 1 1-5.064-5.073h.708V8.64a2.298 2.298 0 1 0 1.617 2.205L8.423 0Z" fill="currentColor"/></svg>
                      {% comment %}Icon tiktok:default:1.0.0-pre.0 end{% endcomment %}
                    </a>
                  </li>
                {% endif %}
              
                {% if settings.social_snapchat_link != '' %}
                  <li class="social-media-links__item">
                    <a
                      class="social-media-links__link"
                      href="{{ settings.social_snapchat_link }}"
                      aria-label="{{ shop.name }} on Snapchat"
                      rel="me"
                      target="_blank"
