{% liquid
  assign nav_position = 'left-left'
  assign sticky = section.settings.header_sticky | default: false
  assign show_border = section.settings.show_border | default: false
  assign logo = section.settings.header_logo | default: false
  assign logo_mobile = section.settings.mobile_header_logo | default: false
  assign logo_width = section.settings.logo_width | default: '300'
  assign logo_width_mobile = section.settings.logo_width_mobile | default: logo_width
  assign logo_width_size = section.settings.logo_width | append: 'x'
  assign logo_width_mobile_size = section.settings.logo_width_mobile | append: 'x'
  assign show_account = section.settings.show_account | default: true
  assign announcement_blocks = section.blocks | where: 'type', 'announcement'
%}

<section
  data-component="header-password"
  data-component-type="section"
  {% if tag != blank %}data-component-tag="{{ tag }}"
  {% endif %}class="header-password header-password--nav-position-{{ nav_position }}"
  data-header-password-section
  data-is-sticky="{{ sticky }}"
  data-show-separator="{{ show_border }}"
  tabindex="-1"
>
  <template data-drawer>
    {% comment %}Component header-password-drawer:default:1.0.0-pre.10 begin{% endcomment %}
    <div data-component="header-password-drawer" data-component-type="component" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="header-password-drawer">
      <div class="header-password-drawer__header">
        <h2 class="header-password-drawer__heading">{{ 'header_password.login.login_form_heading' | t }}</h2>
        <button class="header-password-drawer__close" aria-label="{{ 'general.close' | t }}" data-header-password-drawer-close>
          {% comment %}Icon close:default:1.0.0-pre.0 begin{% endcomment %}
          <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="m1 1 8 8m8-8L9 9m8 8L9 9m-8 8 8-8" stroke="currentColor" stroke-width="1.75"/></svg>
          {% comment %}Icon close:default:1.0.0-pre.0 end{% endcomment %}
        </button>
      </div>
    
      {%- form 'storefront_password', class: 'header-password-drawer__content' -%}
        {% assign label_text = 'header_password.login.login_form_password_placeholder' | t %}
        {% capture field_attributes %}
          autocomplete="current-password"
          {% if form.errors %}
            aria-invalid="true"
            aria-describedby="header-password-drawer__password-error"
          {% endif %}
        {% endcapture %}
        {% comment %}Component input-field:default:1.0.0-pre.10 begin{% endcomment %}
        {% liquid
          assign input_type_5799330d = input_type
          assign input_type = 'password'
          assign field_name_5799330d = field_name
          assign field_name = 'password'
          assign attributes_5799330d = attributes
          assign attributes = field_attributes
        %}
        {% comment %}
          @param form [Required]
            The form that the input lives in.
        
          @param form_namespace [Required]
            The form's namespace that is used in "name" attribute as form_namespace[field_name].
        
          @param field_name [Required]
            The field name for the form. Will be used for the "name" attribute.
        
          @param input_type [Required]
            The type of field that is being rendered. Usually text, password, number, etc.
        
          @param input_id [Optional]
            The type of field that is being rendered. Usually text, password, number, etc.
        
          @param label_text [Required]
            The text for the label to be included.
        
          @param placeholder [Optional]
            The placeholder for the input field.
        
          @param attributes [Optional]
            Additional attributes to append to the input field.
        {% endcomment %}
        
        {% if input_id == blank %}
          {% comment %}UUID input_id begin{% endcomment %}
          {% assign input_id = 'd9e954e8-78d9-40a9-b092-a92c89a82960' %}
          {% comment %}UUID input_id end{% endcomment %}
        {% endif %}
        
        <div data-component="input-field" data-component-type="component" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="input-field">
          <label for="{{ input_id }}">{{ label_text }}</label>
          <input
            id="{{ input_id }}"
            type="{{ input_type }}"
            name="{% if form_namespace %}{{ form_namespace }}[{{ field_name }}]{% else %}{{ field_name }}{% endif %}"
            value="{{ form[field_name] }}"
            placeholder="{{ placeholder }}"
            {{ attributes }}
          >
        </div>
        
        {% liquid
          assign input_type = input_type_5799330d
          assign input_type_5799330d = nil
          assign field_name = field_name_5799330d
          assign field_name_5799330d = nil
          assign attributes = attributes_5799330d
          assign attributes_5799330d = nil
        %}
        {% comment %}Component input-field:default:1.0.0-pre.10 end{% endcomment %}
    
        {% assign login_text = 'header_password.login.login_form_submit' | t %}
        {% comment %}Component button:default:1.0.0-pre.10 begin{% endcomment %}
        {% liquid
          assign style_efdc0705 = style
          assign style = 'primary'
          assign size_efdc0705 = size
          assign size = 'medium'
          assign text_efdc0705 = text
          assign text = login_text
        %}
        {% comment %}
          @param style [Required]
            This is the style of the button: primary, secondary, tertiary, or text. Default primary.
        
          @param size [Required]
            This defines the size of the button: small, medium, or large. Default medium.
        
          @param text [Required]
            This is the text to be included in the button.
        
          @param attributes [Optional]
            This will include any additional attributes that the button needs. For example, aria-controls.
        {% endcomment %}
        
        <button
          data-component="button"
          data-component-type="component"
          {% if tag != blank %}data-component-tag="{{ tag }}"
          {% endif %}class="button button--{{ style }} button--{{ size }}{% if style == 'primary' or style == 'secondary' %} button--shadow{% endif %%}"
          {{ attributes }}
        >
          {{ text }}
        </button>
        
        {% liquid
          assign style = style_efdc0705
          assign style_efdc0705 = nil
          assign size = size_efdc0705
          assign size_efdc0705 = nil
          assign text = text_efdc0705
          assign text_efdc0705 = nil
        %}
        {% comment %}Component button:default:1.0.0-pre.10 end{% endcomment %}
    
        {%- if form.errors -%}
          <div id="header-password-drawer__password-error" class="header-password-drawer__password-error" role="status">
            <span class="form__message">{{ 'header_password.login.login_form_error' | t }}</span>
          </div>
        {%- endif -%}
    
        <small class="header-password-drawer__footer-text">{{ 'header_password.login.admin_link_html' | t }}</small>
      {%- endform -%}
    </div>
    
    {% comment %}Component header-password-drawer:default:1.0.0-pre.10 end{% endcomment %}
  </template>

  {% if sticky %}
    <style>
      .section-header-password {
        position: sticky;
        top: calc(var(--announcement-height, 0px) * -1);
      }
    </style>
  {% endif %}

  {% liquid
    assign password_error = false
    capture _
      form 'storefront_password'
        if form.errors
          assign password_error = true
        endif
      endform
    endcapture
  %}
  <script
  type="application/json"
  data-component-data
  >
    {
      "password_error": {{ password_error | json }},
      "settings": {
        "sticky": {{ sticky | json }},
        "show_border": {{ show_border | json }},
        "announcement_blocks_size": {{ announcement_blocks.size | json }}
      }
    }
  </script>

  <section-wrapper width="wide">
    {% if
      section.settings.show_social_icons
      or announcement_blocks.size > 0
      or section.settings.enable_language_selector
      or section.settings.enable_country_selector
    %}
      {% comment %}Component header-toolbar:default:1.0.0-pre.10 begin{% endcomment %}
      {% assign announcement_blocks = section.blocks | where: 'type', 'announcement' %}
      
      <div
        data-component="header-tool-bar"
        data-component-type="component"
        {% if tag != blank %}data-component-tag="{{ tag }}"
        {% endif %}class="
          header-toolbar
          {% if section.blocks.size == 0 %}
            header-toolbar--no-mobile-content
          {% endif %}
        "
        style="
          --toolbar-text-color: {{ section.settings.toolbar_text_color }};
          --toolbar-text-color-transparent: {{ section.settings.toolbar_text_color | color_modify: 'alpha', 0.15 }};
          --toolbar-bg-color: {{ section.settings.toolbar_bg_color }};
        "
      >
        <script
          type="application/json"
          data-component-data
        >
          {
            "settings": {
              "block_size": {{ announcement_blocks.size | json }}
            }
          }
        </script>
        <div class="header-toolbar__content">
          <div class="header-toolbar__content-links">
            {% if section.settings.show_social_icons %}
              {% comment %}Component social-media-links:default:1.0.0-pre.10 begin{% endcomment %}
