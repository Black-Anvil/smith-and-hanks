                      {% comment %}Icon snapchat:default:1.0.0-pre.0 end{% endcomment %}
                    </a>
                  </li>
                {% endif %}
              
                {% if settings.social_tumblr_link != '' %}
                  <li class="social-media-links__item">
                    <a
                      class="social-media-links__link"
                      href="{{ settings.social_tumblr_link }}"
                      aria-label="{{ shop.name }} on Tumblr"
                      rel="me"
                      target="_blank"
                    >
                      {% comment %}Icon tumblr:default:1.0.0-pre.0 begin{% endcomment %}
                      <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" clip-rule="evenodd" d="M10.062 16c-2.66 0-4.656-1.235-4.656-4.198V7.06H3V4.49C5.66 3.867 6.77 1.788 6.902 0h2.761v4.068h3.215V7.06H9.663v4.128c0 1.235.687 1.667 1.785 1.667H13V16h-2.938Z" fill="currentColor"/></svg>
                      {% comment %}Icon tumblr:default:1.0.0-pre.0 end{% endcomment %}
                    </a>
                  </li>
                {% endif %}
              </ul>
              
              {% comment %}Component social-media-links:default:1.0.0-pre.10 end{% endcomment %}
            {% endif %}
          </div>
          {% if announcement_blocks.size > 0 %}
            <div
              class="header-toolbar__content-announcement"
              data-announcement-slider
            >
              <ul
                class="header-toolbar__slides"
                aria-label="{{ section.slideshow.title | t }}"
                tabindex="0"
                data-header-toolbar-list
              >
                {% for block in announcement_blocks %}
                  {% if block.settings.text != '' %}
                    {% comment %}Component block-announcement:default:1.0.0-pre.10 begin{% endcomment %}
                    <li
                      data-component="block-announcement"
                      data-component-type="block"
                      {% if tag != blank %}data-component-tag="{{ tag }}"
                      {% endif %}id="header-toolbar-{{ section.id }}-slide-{{ forloop.index }}"
                      class="block-announcement"
                      data-announcement-block
                      data-slide-index="{{ forloop.index }}"
                      {{ block.shopify_attributes }}
                    >
                    
                      {% assign url = block.settings.url %}
                      {% unless url == nil or block.settings.text contains '</a>' %}
