{% liquid
  assign nav_position = section.settings.nav_position | default: 'left-left'
  assign sticky = section.settings.header_sticky | default: false
  assign show_border = section.settings.show_border | default: false
  assign logo = section.settings.header_logo | default: false
  assign logo_mobile = section.settings.mobile_header_logo | default: false
  assign logo_width = section.settings.logo_width | default: '300'
  assign logo_width_mobile = section.settings.logo_width_mobile | default: logo_width
  assign logo_width_size = section.settings.logo_width | append: 'x'
  assign logo_width_mobile_size = section.settings.logo_width_mobile | append: 'x'
  assign show_account = section.settings.show_account | default: true
  assign announcement_blocks = section.blocks | where: 'type', 'announcement'
  assign show_header_toolbar = false
%}

<section
  data-component="header"
  data-component-type="section"
  {% if tag != blank %}data-component-tag="{{ tag }}"
  {% endif %}class="header header--nav-position-{{ nav_position }}"
  data-header-section
  data-is-sticky="{{ sticky }}"
  data-show-separator="{{ show_border }}"
  tabindex="-1"
>
  {% if sticky %}
    <style>
      .section-header {
        position: sticky;
        top: calc(var(--announcement-height, 0px) * -1);
      }
    </style>
  {% endif %}
  <script
  type="application/json"
  data-component-data
  >
    {
      "settings": {
        "sticky": {{ sticky | json }},
        "show_border": {{ show_border | json }},
        "announcement_blocks_size": {{ announcement_blocks.size | json }},
        "enable_live_search": {{ settings.enable_live_search | json }}
      }
    }
  </script>

  <section-wrapper width="wide">
    {% if announcement_blocks.size > 0
      or section.settings.enable_language_selector
      or section.settings.enable_country_selector
    %}
      {% assign show_header_toolbar = true %}
    {% elsif section.settings.show_social_icons %}
      {% if settings.social_twitter_link != blank
        or settings.social_facebook_link != blank
        or settings.social_pinterest_link != blank
        or settings.social_instagram_link != blank
        or settings.social_tiktok_link != blank
        or settings.social_tumblr_link != blank
        or settings.social_snapchat_link != blank
        or settings.social_youtube_link != blank
        or settings.social_vimeo_link != blank
      %}
        {% assign show_header_toolbar = true %}
      {% endif %}
    {% endif %}
    {% if show_header_toolbar %}
      {% comment %}Component header-toolbar:default:1.0.0-pre.10 begin{% endcomment %}
      {% assign announcement_blocks = section.blocks | where: 'type', 'announcement' %}
      
      <div
        data-component="header-tool-bar"
        data-component-type="component"
        {% if tag != blank %}data-component-tag="{{ tag }}"
        {% endif %}class="
          header-toolbar
          {% if section.blocks.size == 0 %}
            header-toolbar--no-mobile-content
          {% endif %}
        "
        style="
          --toolbar-text-color: {{ section.settings.toolbar_text_color }};
          --toolbar-text-color-transparent: {{ section.settings.toolbar_text_color | color_modify: 'alpha', 0.15 }};
          --toolbar-bg-color: {{ section.settings.toolbar_bg_color }};
        "
      >
        <script
          type="application/json"
          data-component-data
        >
          {
            "settings": {
              "block_size": {{ announcement_blocks.size | json }}
            }
          }
        </script>
        <div class="header-toolbar__content">
          <div class="header-toolbar__content-links">
            {% if section.settings.show_social_icons %}
              {% comment %}Component social-media-links:default:1.0.0-pre.10 begin{% endcomment %}
              {% comment %}
                @param use_mobile_spacing {Boolean}} [Optional]
                  Changes the spacing to allow for bigger touch targets on mobile if true.
              {% endcomment %}
              
              {% assign use_mobile_spacing = use_mobile_spacing | default: false %}
              
              <ul data-component="social-media-links" data-component-type="component" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="social-media-links {% if use_mobile_spacing %}social-media-links--mobile-spacing{% endif %}">
                {% if settings.social_facebook_link != '' %}
                  <li class="social-media-links__item">
                    <a
                      class="social-media-links__link"
                      href="{{ settings.social_facebook_link }}"
                      aria-label="{{ shop.name }} on Facebook"
                      rel="me"
                      target="_blank"
                    >
                      {% comment %}Icon facebook:default:1.0.0-pre.0 begin{% endcomment %}
