                  <li class="social-media-links__item">
                    <a
                      class="social-media-links__link"
                      href="{{ settings.social_youtube_link }}"
                      aria-label="{{ shop.name }} on YouTube"
                      rel="me"
                      target="_blank"
                    >
                      {% comment %}Icon youtube:default:1.0.0-pre.0 begin{% endcomment %}
