{% liquid
  assign is_double_image = false
  if section.settings.image and section.settings.image_2
    assign is_double_image = true
  endif

  assign has_mobile_image = false
  if section.settings.mobile_image
    assign has_mobile_image = true
  endif

  assign desktop_height = 'auto'

  if section.settings.desktop_height != 'adapt'
    assign desktop_height = section.settings.desktop_height | append: 'px'
  endif

  assign image_1_focal_point = section.settings.image_1_focal_point | replace: '_', ' '
  assign image_2_focal_point = section.settings.image_2_focal_point | replace: '_', ' '
  assign image_mobile_focal_point = section.settings.image_mobile_focal_point | replace: '_', ' '

  if section.settings.color_overlay_opacity > 0
    assign overlay_opacity = section.settings.color_overlay_opacity | times: 0.01
  endif

  if section.settings.overlay_color != blank and section.settings.overlay_color != 'rgba(0,0,0,0)'
    assign overlay_color = section.settings.overlay_color | color_modify: 'alpha', overlay_opacity
  endif

  if section.settings.overlay_gradient != blank
    assign overlay_color = section.settings.overlay_gradient
  endif

  assign top_level_blocks = nil | sort
  assign center_level_blocks = nil | sort
  assign bottom_level_blocks = nil | sort

  if section.blocks.size > 0
    for block in section.blocks
      assign position = block.settings.position | split: '_'
      assign block_to_add = block | sort
      case position[0]
        when 'top'
          assign top_level_blocks = top_level_blocks | concat: block_to_add
        when 'center'
          assign center_level_blocks = center_level_blocks | concat: block_to_add
        when 'bottom'
          assign bottom_level_blocks = bottom_level_blocks | concat: block_to_add
      endcase
    endfor
  endif

  assign text_color = section.settings.color_overlay_foreground
  assign has_text_color = true
  assign text_color_alpha = text_color | color_extract: 'alpha'

  if text_color_alpha == 0
    assign has_text_color = false
  endif
%}

{% capture overlay_content_properties %}
  {% if overlay_color != blank %}
    --overlay-color: {{ overlay_color }};
  {% endif %}
  {% if has_text_color %}
    --local-overlay-text-color: {{ section.settings.color_overlay_foreground }};
  {% endif %}
    --block-text-color: var(--local-overlay-text-color, var(--color-overlay-text));
    --text-alignment: {{ section.settings.text_alignment }};
{% endcapture %}

<section data-component="image-banner" data-component-type="section" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="image-banner">
  <script
    type="application/json"
    data-component-data
  >
    {
      "desktopHeight": {{ section.settings.desktop_height | json }}
    }
  </script>
  <section-wrapper {% if section.settings.wide %}width="wide"{% endif %}>
    <section-content>
      <div
        class="
          image-banner__wrapper
          {% if is_double_image %}image-banner__wrapper--double-image{% endif %}
        "
        style="--desktop-height: {{ desktop_height }};"
        data-image-banner-wrapper
      >
        {% if section.settings.image %}
          <div
            class="
              image-banner__image
              {% if section.settings.image_2 %}image-banner__image--half{% endif %}
              {% if has_mobile_image %}image-banner__image--mobile-hidden{% endif %}
            "
            style="
              --focal-point: {{ image_1_focal_point }};
            "
            data-image-banner-image
          >
            {% comment %}Component lazy-img:default:1.0.0-pre.10 begin{% endcomment %}
            {% render 'lazy-img', img: section.settings.image, lazy: true, canvas: true %}
            {% comment %}Component lazy-img:default:1.0.0-pre.10 end{% endcomment %}
          </div>
        {% elsif section.settings.image_2 == blank %}
          <div class="image-banner__image" data-image-banner-image>
            {{ 'lifestyle-2' | placeholder_svg_tag: 'placeholder-svg' }}
          </div>
        {% endif %}

        {% if section.settings.image_2 %}
          <div
            class="
              image-banner__image
              image-banner__image--second
              {% if section.settings.image %}image-banner__image--half{% endif %}
            "
            style="--focal-point: {{ image_2_focal_point }};"
            data-image-banner-image
          >
            {% comment %}Component lazy-img:default:1.0.0-pre.10 begin{% endcomment %}
            {% render 'lazy-img', img: section.settings.image_2, lazy: true, canvas: true %}
            {% comment %}Component lazy-img:default:1.0.0-pre.10 end{% endcomment %}
          </div>
        {% endif %}

        {% if section.settings.mobile_image %}
          <div
            class="
              image-banner__image
              image-banner__image--mobile
            "
            style="--focal-point: {{ image_mobile_focal_point }};"
            data-image-banner-image
          >
            {% comment %}Component lazy-img:default:1.0.0-pre.10 begin{% endcomment %}
            {% render 'lazy-img', img: section.settings.mobile_image, lazy: true, canvas: true %}
            {% comment %}Component lazy-img:default:1.0.0-pre.10 end{% endcomment %}
          </div>
        {% endif %}

        <div
          class="
            image-banner__overlay
          "
          style="{{ overlay_content_properties }}"
        >
          {% if section.blocks.size > 0 %}
            <div
              class="
                image-banner__content-grid
                image-banner__content-grid--{{ section.settings.text_alignment }}-aligned
                {% if section.settings.wide %}
