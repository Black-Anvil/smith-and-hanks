                    {% if block.settings.heading != blank %}
                      <h2 class="block-footer-about__heading">
                        {{ block.settings.heading | escape }}
                      </h2>
                    {% endif %}
                  
                    {% if block.settings.content != blank %}
                      <div class="block-footer-about__content">
                        {{ block.settings.content }}
                      </div>
                    {% endif %}
                    {% if block.settings.show_social_icons %}
                      {% comment %}Component social-media-links:default:1.0.0-pre.10 begin{% endcomment %}
                      {% comment %}
                        @param use_mobile_spacing {Boolean}} [Optional]
                          Changes the spacing to allow for bigger touch targets on mobile if true.
                      {% endcomment %}
                      
                      {% assign use_mobile_spacing = use_mobile_spacing | default: false %}
                      
                      <ul data-component="social-media-links" data-component-type="component" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="social-media-links {% if use_mobile_spacing %}social-media-links--mobile-spacing{% endif %}">
                        {% if settings.social_facebook_link != '' %}
                          <li class="social-media-links__item">
                            <a
                              class="social-media-links__link"
                              href="{{ settings.social_facebook_link }}"
                              aria-label="{{ shop.name }} on Facebook"
                              rel="me"
                              target="_blank"
                            >
                              {% comment %}Icon facebook:default:1.0.0-pre.0 begin{% endcomment %}
                              <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" clip-rule="evenodd" d="M7.591.077a7.981 7.981 0 0 0-5.247 2.329A7.925 7.925 0 0 0 .392 5.573a8 8 0 0 0 .248 5.604c.072.17.254.543.347.71.243.44.542.876.862 1.258a7.984 7.984 0 0 0 4.886 2.78l.016.002v-5.576H4.719V8.04H6.75L6.753 7c.003-1.107.002-1.082.04-1.341.138-.952.594-1.67 1.327-2.084.336-.19.758-.319 1.211-.369.46-.05 1.219-.012 2.02.102l.212.03c.002 0 .003.441.003.982v.983l-.598.003c-.64.003-.669.005-.838.044-.437.101-.728.384-.832.806-.041.17-.042.185-.042 1.07v.814h1.106c.999 0 1.107.002 1.107.012 0 .013-.343 2.244-.35 2.279l-.004.02H9.256v5.573l.04-.005a7.9 7.9 0 0 0 2.286-.738 8.013 8.013 0 0 0 3.837-4.142c.33-.813.517-1.647.572-2.561.012-.186.012-.679 0-.864a7.95 7.95 0 0 0-.497-2.37A8.02 8.02 0 0 0 10.9.607 8.088 8.088 0 0 0 8.532.084a16.32 16.32 0 0 0-.941-.007Z" fill="currentColor"/></svg>
                              {% comment %}Icon facebook:default:1.0.0-pre.0 end{% endcomment %}
