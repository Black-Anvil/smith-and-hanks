                            <a
                              class="social-media-links__link"
                              href="{{ settings.social_twitter_link }}"
                              aria-label="{{ shop.name }} on Twitter"
                              rel="me"
                              target="_blank"
                            >
                              {% comment %}Icon twitter:default:1.0.0-pre.0 begin{% endcomment %}
