<section data-component="main-cart" data-component-type="section" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="cart">
  <script
    type="application/json"
    data-component-data
  >
    {
      "locales": {
        "error": {{ 'templates.cart.cart_error' | t | json }},
        "quantityError": {{ 'templates.cart.cart_quantity_error_html' | t | json }}
      }
    }
  </script>
  <section-wrapper width="content">
    <section-header>
      <div class="cart__header">
        <h1 class="cart__heading">{{ 'templates.cart.title' | t }} {% if cart != empty %}(<span data-header-cart-count>{{ cart.item_count }}</span>){% endif %}</h1>
      </div>
    </section-header>
  </section-wrapper>
  <section-wrapper width="content">
    <section-content>
      <div
        class="cart-page"
        data-cart-page=""
        data-id="{{ section.id }}"
      >
        {% if cart != empty %}
          <div
            class="cart__errors"
            role="alert"
            data-cart-errors
          >
          </div>

          <form
            class="cart__form"
            id="cart"
            action="{{ routes.cart_url }}"
            method="post"
            data-cart-form
            data-cart-count="{{ cart.item_count }}"
          >
            <div
              id="cart-contents"
              data-cart-contents
            >
              <div
                class="cart-contents__wrapper"
                data-cart-items
              >

                  <table class="cart-items">
                    <thead>
                      <tr class="cart-items__headings">
                        <th
                          class="cart-items__heading-title"
                          colspan="2"
                          scope="col"
                        >
                          {{ 'templates.cart.headings.product' | t }}
                        </th>
                        <th
                          class="cart-items__heading-quantity"
                          colspan="1"
                          scope="col"
                        >
                          {{ 'templates.cart.headings.quantity' | t }}
                        </th>
                        <th
                          class="cart-items__heading-total"
                          colspan="1"
                          scope="col"
                        >
                          {{ 'templates.cart.headings.total' | t }}
                        </th>
                      </tr>
                    </thead>

                    <tbody>
                      {%- for cart_item in cart.items -%}
                        <tr
                          class="cart-item"
                          id="{{ forloop.index }}"
                          data-cart-row="{{ forloop.index }}"
                          data-cart-item-key="{{ cart_item.key }}"
                        >
                          <td class="cart-item__image--mobile">
                            {%- if cart_item.image -%}
                              <a
                                class="cart-item__image-wrapper--mobile"
                                href="{{ cart_item.url }}"
                              >
                                {% comment %}Component lazy-img:default:1.0.0-pre.10 begin{% endcomment %}
                                {% render 'lazy-img', img: cart_item.image, size: '600x', lazy: true, canvas: true %}
                                {% comment %}Component lazy-img:default:1.0.0-pre.10 end{% endcomment %}
                              </a>
                            {%- endif -%}
                          </td>
                          <td class="cart-item__details">
                            <div class="cart-item__details-image">
                              {%- if cart_item.image -%}
                                <a
                                  class="cart-item__image-wrapper"
                                  href="{{ cart_item.url }}"
                                >
                                  {% comment %}Component lazy-img:default:1.0.0-pre.10 begin{% endcomment %}
                                  {% render 'lazy-img', img: cart_item.image, size: '1200x', lazy: true, canvas: true %}
                                  {% comment %}Component lazy-img:default:1.0.0-pre.10 end{% endcomment %}
                                </a>
                              {%- endif -%}
                            </div>
                            <div class="cart-item__details-info">
                              <h3 class="cart-item__heading">
                                <a href="{{ cart_item.url }}">
                                  {{ cart_item.product.title }}
                                </a>
                              </h3>

                              {%- if cart_item.product.has_only_default_variant == false or cart_item.properties.size != 0 -%}
                                <dl class="cart-item__options">
                                  {%- if cart_item.product.has_only_default_variant == false -%}
                                    {%- for option in cart_item.options_with_values -%}
                                      <div class="cart-item__option">
                                        <dt>{{ option.name }}: </dt>
                                        <dd>{{ option.value }}</dd>{% unless forloop.last %}<span class="option-seperator">/</span>{% endunless %}
                                      </div>
                                    {%- endfor -%}
                                  {%- endif -%}

                                  {%- for property in cart_item.properties -%}
                                    {%- assign property_first_char = property.first | slice: 0 -%}
                                    {%- if property.last != blank and property_first_char != '_' -%}
                                      <div class="cart-item__option">
                                        <dt>{{ property.first }}: </dt>
                                        <dd>
                                          {%- if property.last contains '/uploads/' -%}
                                            <a href="{{ property.last }}" target="_blank">
                                              {{ property.last | split: '/' | last }}
                                            </a>
                                          {%- else -%}
                                            {{ property.last }}
                                          {%- endif -%}
                                        </dd>{% unless forloop.last %}<span class="option-seperator">/</span>{% endunless %}
                                      </div>
                                    {%- endif -%}
                                  {%- endfor -%}

                                  {%- if cart_item.selling_plan_allocation != nil -%}
                                    {% assign optionSize = cart_item.options_with_values | size %}
                                    {% assign propertySize = cart_item.properties | size %}
                                    {% if propertySize > 0 or optionSize > 0 %}
                                      <span {{ cart_item.properties.length }} class="option-seperator">/</span>
                                    {% endif %}
                                    <div class="cart-item__plan">
                                      {% for option in cart_item.selling_plan_allocation.selling_plan.options %}
                                        <dt>{{ option.name }}:</dt>
                                        <dd>{{ option.value }}</dd>
                                      {% endfor %}
                                    </div>
                                  {%- endif -%}
                                </dl>
                              {%- endif -%}

                              {% if settings.cart_show_vendor %}
                                <p class="cart-item__vendor">{{ cart_item.product.vendor }}</p>
                              {% endif %}

                              {% if cart_item.discounts.size > 0 %}
                                <ul
                                  class="cart-item__discounts"
                                  role="list"
                                  aria-label="{{ 'customer.order.discount' | t }}"
                                >
                                  {%- for discount in cart_item.discounts -%}
                                    <li class="cart-item__discount">
                                      <span class="cart-item__discount-title">{{ discount.title }}: -{{ discount.total_amount | money }}</span>
                                    </li>
                                  {%- endfor -%}
                                </ul>
                              {% endif %}

                              <div
                                class="cart-item__error"
                                data-cart-item-error="{{ cart_item.key}}"
                              >
                              </div>
                            </div>

                          </td>

                          <td class="cart-item__quantity">
                            <div class="cart-item__quantity-wrapper">
                              {% assign tag = cart_item.key%}

                              {% comment %}Component quantity-selector:default:1.0.0-pre.10 begin{% endcomment %}
                              {% liquid
                                assign name_2227616c = name
                                assign name = 'updates[]'
                                assign quantity_2227616c = quantity
                                assign quantity = cart_item.quantity
                                assign title_2227616c = title
                                assign title = cart_item.product.title
                              %}
                              {% comment %}
                                @param quantity [Required]
                                  The default quantity to be set.
                              
                                @param title [Required]
                                  The title of the quantity selectors product
                                @param name
                                @param [form_id]
                                @param [min]
                              {% endcomment %}
                              
                              {% liquid
                                unless min
                                  assign min = 0
                                endunless
                              %}
                              
                              <div data-component="quantity-selector" data-component-type="component" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="quantity-selector">
                                <button
                                  class="quantity-selector__button quantity-selector__button--minus"
                                  name="minus"
                                  type="button"
                                  data-quantity-minus
                                >
                                  <span class="visually-hidden">{{ 'general.components.quantity_selector.decrease' | t: title: title | escape }}</span>
                                </button>
                                <input
                                  class="quantity-selector__input"
                                  type="number"
                                  name="{{ name }}"
                                  {% if form_id != blank %}form="{{ form_id }}"{% endif %}
                                  value="{{ quantity }}"
                                  min="{{ min }}"
                                  aria-label="{{ 'general.components.quantity_selector.input_label' | t: title: title | escape }}"
                                  data-quantity-input
                                >
                                <button
                                  class="quantity-selector__button quantity-selector__button--plus"
                                  name="plus"
                                  type="button"
                                  data-quantity-plus
                                >
                                  <span class="visually-hidden">{{ 'general.components.quantity_selector.increase' | t: title: title | escape }}</span>
                                </button>
                              </div>
                              
                              {% liquid
                                assign name = name_2227616c
                                assign name_2227616c = nil
                                assign quantity = quantity_2227616c
                                assign quantity_2227616c = nil
                                assign title = title_2227616c
                                assign title_2227616c = nil
                              %}
                              {% comment %}Component quantity-selector:default:1.0.0-pre.10 end{% endcomment %}

                              <a
                                class="cart-item__remove"
                                href="{{ cart_item.url_to_remove }}"
                                name="remove"
                                aria-label="{{ 'templates.cart.remove_title' | t: title: cart_item.title }}"
                                data-remove
                              >
                                {{ 'templates.cart.remove' | t }}
                              </a>
                            </div>
                          </td>

                          <td class="cart-item__price">
                            {% assign compare_at_line_price = cart_item.variant.compare_at_price | times: cart_item.quantity %}
                            {%- if cart_item.final_line_price < compare_at_line_price -%}
                              <dl>
                                <dt class="visually-hidden">
                                  {{ 'products.product.price.sale_price' | t }}
                                </dt>
                                <dd class="cart-drawer-item__sale-price">
                                  {{ cart_item.final_line_price | money }}
                                </dd>
                                <dt class="visually-hidden">
                                  {{ 'products.product.price.regular_price' | t }}
                                </dt>
                                <dd class="cart-drawer-item__price-original">
                                  <s>
                                    {{ compare_at_line_price | money }}
                                  </s>
                                </dd>
                              </dl>
                            {%- elsif cart_item.final_line_price < cart_item.original_line_price %}
                              <dl>
                                <dt class="visually-hidden">
                                  {{ 'products.product.price.sale_price' | t }}
                                </dt>
                                <dd class="cart-drawer-item__sale-price">
                                  {{ cart_item.final_line_price | money }}
                                </dd>
                                <dt class="visually-hidden">
                                  {{ 'products.product.price.regular_price' | t }}
                                </dt>
                                <dd class="cart-drawer-item__price-original">
                                  <s>
                                    {{ cart_item.original_line_price | money }}
                                  </s>
                                </dd>
                              </dl>
                            {%- else -%}
                              <span>
                                {{ cart_item.final_line_price | money }}
                              </span>
                            {%- endif -%}

                            {%- if cart_item.variant.available and cart_item.unit_price_measurement -%}
                              <div class="cart-item__unit-price">
                                <span class="visually-hidden">{{ 'products.product.price.unit_price' | t }}</span>
                                {{ cart_item.variant.unit_price | money }}
                                <span aria-hidden="true">/</span>
                                <span class="visually-hidden">&nbsp;{{ 'accessibility.unit_price_separator' | t }}&nbsp;</span>
                                {%- if cart_item.variant.unit_price_measurement.reference_value != 1 -%}
                                  {{- cart_item.variant.unit_price_measurement.reference_value -}}
                                {%- endif -%}
                                {% if cart_item.variant.unit_price_measurement.reference_unit == 'm2' %}
                                  m<sup>2</sup>
                                {% elsif cart_item.variant.unit_price_measurement.reference_unit == 'm3' %}
                                  m<sup>3</sup>
                                {% else %}
                                  {{ cart_item.variant.unit_price_measurement.reference_unit }}
                                {% endif %}
                              </div>
                            {%- endif -%}
                          </td>
                        </tr>
                      {%- endfor -%}
                    </tbody>
                  </table>

              </div>
            </div>
          </form>

          <div class="cart__bottom">
            <div class="cart__bottom-notes">
              {%- if settings.cart_show_note -%}
                {% assign placeholder = 'templates.cart.note' | t %}
                {% assign cart_note = cart.note | strip %}
                {% comment %}Component input-textarea:default:1.0.0-pre.10 begin{% endcomment %}
                {% liquid
                  assign label_text_ab38ecb9 = label_text
                  assign label_text = placeholder
                  assign field_name_ab38ecb9 = field_name
                  assign field_name = 'note'
                  assign form_id_ab38ecb9 = form_id
                  assign form_id = 'cart'
                  assign input_text_ab38ecb9 = input_text
                  assign input_text = cart_note
                  assign placeholder_ab38ecb9 = placeholder
                  assign placeholder = ''
                %}
                {% comment %}
                  @param label_text [Required]
                    The text for the label to be included.
                
                  @param form_namespace [Required]
                    The form's namespace that is used in "name" attribute as form_namespace[field_name].
                
                  @param field_name [Required]
                    The field name for the form. Will be used for the "name" attribute.
                
                  @param form_id [Optional]
                    The form that the input lives in.
                
                  @param input_id [Optional]
                    The type of field that is being rendered. Usually text, password, number, etc.
                
                  @param input_text [Optional]
                    The initial text to display within the textarea
                
                  @param placeholder [Optional]
                    The placeholder for the input field.
                
                  @param attributes [Optional]
                    Additional attributes to append to the input field.
                
                  @param rows [Optional]
                    The number of rows for the textarea (height)
                
                  @param columns [Optional]
                    The number of columns for the textarea (width)
                {% endcomment %}
                
                {% if input_id == blank %}
                  {% comment %}UUID input_id begin{% endcomment %}
                  {% assign input_id = '14fcfa4c-a585-493c-8beb-2d84b5132d2a' %}
                  {% comment %}UUID input_id end{% endcomment %}
                {% endif %}
                
                <div data-component="input-textarea" data-component-type="component" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="textarea">
                  <label for="{{ input_id }}">{{ label_text }}</label>
                  <textarea
                    id="{{ input_id }}"
                    name="{% if form_namespace %}{{ form_namespace }}[{{ field_name }}]{% else %}{{ field_name }}{% endif %}"
                    {% if form_id %}
                      form="{{ form_id }}"
                    {% endif %}
                    {% if rows %}rows="{{ rows }}"{% endif %}
                    {% if columns %}cols="{{ columns }}"{% endif %}
                    placeholder="{{ placeholder }}"
                    {{ attributes }}
                    data-input
                  >
                    {%- if input_text != blank -%}{{- input_text -}}{%- endif -%}
                  </textarea>
                </div>
                
                {% liquid
                  assign label_text = label_text_ab38ecb9
                  assign label_text_ab38ecb9 = nil
                  assign field_name = field_name_ab38ecb9
                  assign field_name_ab38ecb9 = nil
                  assign form_id = form_id_ab38ecb9
                  assign form_id_ab38ecb9 = nil
                  assign input_text = input_text_ab38ecb9
                  assign input_text_ab38ecb9 = nil
                  assign placeholder = placeholder_ab38ecb9
                  assign placeholder_ab38ecb9 = nil
                %}
                {% comment %}Component input-textarea:default:1.0.0-pre.10 end{% endcomment %}
              {%- endif -%}

            </div>
            <div class="cart__bottom-totals">
              <div
                class="cart__bottom-total"
                data-cart-total
              >
                <div>
                  {%- if cart.cart_level_discount_applications.size > 0 -%}
                    <ul
                      class="cart__discounts"
                      role="list"
                      aria-label="{{ 'customer.order.discount' | t }}"
                    >
                      {%- for discount in cart.cart_level_discount_applications -%}
                        <li class="cart__discount">
                          <span class="cart__discount-title">{% comment %}Icon tag:default:1.0.0-pre.0 begin{% endcomment %}
<svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M7.32 13.337 2.597 8.622a1.992 1.992 0 0 1-.584-1.402l-.015-3.233a1.974 1.974 0 0 1 .585-1.417 2.006 2.006 0 0 1 1.427-.585l3.369.024a2 2 0 0 1 1.398.584l4.654 4.646a1.995 1.995 0 0 1 0 2.82l-3.285 3.278a1.99 1.99 0 0 1-1.413.585 2.004 2.004 0 0 1-1.413-.585ZM5.652 5.682a.763.763 0 0 0 0-1.08.767.767 0 0 0-1.305.54.763.763 0 0 0 1.305.54Z" fill="currentColor"/></svg>
{% comment %}Icon tag:default:1.0.0-pre.0 end{% endcomment %} {{ discount.title }}:</span>
                          <span class="cart__discount-amount"> -{{ discount.total_allocated_amount | money_with_currency }}</span>
                        </li>
                      {%- endfor -%}
                    </ul>
                  {%- endif -%}
                </div>

                <p class="cart__subtotal">{{ 'templates.cart.subtotal' | t }}: <span class="cart__subtotal-value">{{ cart.total_price | money_with_currency }}</span></p>

                <small class="cart__taxes-policies">
                  {%- if cart.taxes_included and shop.shipping_policy.body != blank -%}
                    {{ 'templates.cart.taxes_included_and_shipping_policy_html' | t: link: shop.shipping_policy.url }}
                  {%- elsif cart.taxes_included -%}
                    {{ 'templates.cart.taxes_included_but_shipping_at_checkout' | t }}
                  {%- elsif shop.shipping_policy.body != blank -%}
                    {{ 'templates.cart.taxes_and_shipping_policy_at_checkout_html' | t: link: shop.shipping_policy.url }}
                  {%- else -%}
                    {{ 'templates.cart.taxes_and_shipping_at_checkout' | t }}
                  {%- endif -%}
                </small>
              </div>
              <noscript class="cart__no-js">
                {% assign update_text = 'templates.cart.update' | t %}
                {% assign update_attributes = 'form="cart" type="submit"' %}
                {% comment %}Component button:default:1.0.0-pre.10 begin{% endcomment %}
                {% liquid
                  assign style_ab7149c2 = style
                  assign style = 'secondary'
                  assign size_ab7149c2 = size
                  assign size = 'medium'
                  assign text_ab7149c2 = text
                  assign text = update_text
                  assign attributes_ab7149c2 = attributes
                  assign attributes = update_attributes
                %}
                {% comment %}
                  @param style [Required]
                    This is the style of the button: primary, secondary, tertiary, or text. Default primary.
                
                  @param size [Required]
                    This defines the size of the button: small, medium, or large. Default medium.
                
                  @param text [Required]
                    This is the text to be included in the button.
                
                  @param attributes [Optional]
                    This will include any additional attributes that the button needs. For example, aria-controls.
                {% endcomment %}
                
                <button
                  data-component="button"
                  data-component-type="component"
                  {% if tag != blank %}data-component-tag="{{ tag }}"
                  {% endif %}class="button button--{{ style }} button--{{ size }}{% if style == 'primary' or style == 'secondary' %} button--shadow{% endif %%}"
                  {{ attributes }}
                >
                  {{ text }}
                </button>
                
                {% liquid
                  assign style = style_ab7149c2
                  assign style_ab7149c2 = nil
                  assign size = size_ab7149c2
                  assign size_ab7149c2 = nil
                  assign text = text_ab7149c2
                  assign text_ab7149c2 = nil
                  assign attributes = attributes_ab7149c2
                  assign attributes_ab7149c2 = nil
                %}
                {% comment %}Component button:default:1.0.0-pre.10 end{% endcomment %}
              </noscript>
              <div class="cart__checkout-buttons">
                {% if settings.enable_checkout_lock_icon %}
                  {% capture lock_icon %}
                    {% comment %}Icon lock:default:1.0.0-pre.0 begin{% endcomment %}
                    <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M12.572 6.286H3.428c-.632 0-1.143.511-1.143 1.143v6.857c0 .63.511 1.143 1.143 1.143h9.143c.63 0 1.142-.512 1.142-1.143V7.429c0-.632-.511-1.143-1.143-1.143Zm-.572 0V4.57a4 4 0 1 0-8 0v1.715" stroke="currentColor" stroke-width="1.143" stroke-linecap="round" stroke-linejoin="round"/><path d="M8 11.429a.571.571 0 1 0 0-1.143.571.571 0 0 0 0 1.143Z" stroke="currentColor" stroke-width="1.143" stroke-linecap="round" stroke-linejoin="round"/></svg>
                    {% comment %}Icon lock:default:1.0.0-pre.0 end{% endcomment %}
                  {% endcapture %}
                {% endif %}
                {% assign checkout_text = 'templates.cart.checkout' | t | prepend: lock_icon %}
                {% assign checkout_attributes = 'type="submit" id="checkout" name="checkout" form="cart"' %}
                {% comment %}Component button:default:1.0.0-pre.10 begin{% endcomment %}
                {% liquid
                  assign style_a80d6504 = style
                  assign style = 'primary'
                  assign size_a80d6504 = size
                  assign size = 'medium'
                  assign text_a80d6504 = text
                  assign text = checkout_text
                  assign attributes_a80d6504 = attributes
                  assign attributes = checkout_attributes
                %}
                {% comment %}
                  @param style [Required]
                    This is the style of the button: primary, secondary, tertiary, or text. Default primary.
                
                  @param size [Required]
                    This defines the size of the button: small, medium, or large. Default medium.
                
                  @param text [Required]
                    This is the text to be included in the button.
                
                  @param attributes [Optional]
                    This will include any additional attributes that the button needs. For example, aria-controls.
                {% endcomment %}
                
                <button
                  data-component="button"
                  data-component-type="component"
                  {% if tag != blank %}data-component-tag="{{ tag }}"
                  {% endif %}class="checkout button button--{{ style }} button--{{ size }}{% if style == 'primary' or style == 'secondary' %} button--shadow{% endif %%}"
                  {{ attributes }}
                >
                  {{ text }}
                </button>
                
                {% liquid
                  assign style = style_a80d6504
                  assign style_a80d6504 = nil
                  assign size = size_a80d6504
                  assign size_a80d6504 = nil
                  assign text = text_a80d6504
                  assign text_a80d6504 = nil
                  assign attributes = attributes_a80d6504
                  assign attributes_a80d6504 = nil
                %}
                {% comment %}Component button:default:1.0.0-pre.10 end{% endcomment %}

                {%- if additional_checkout_buttons and settings.show_dynamic_payment -%}
                  <div class="cart__dynamic-checkout-buttons additional-checkout-buttons">
                    {{ content_for_additional_checkout_buttons }}
                  </div>
                {%- endif -%}
              </div>
            </div>
          </div>
        {% else %}
          <div class="cart__empty">
            <h2 class="cart__empty-text">{{ 'templates.cart.empty' | t }}</h2>
            {% if settings.continue_shopping_collection != blank %}
              {% assign continue_shopping = 'templates.cart.continue_shopping' | t %}
              {% assign collection_url = collections[settings.continue_shopping_collection].url %}
              {% comment %}Component button-link:default:1.0.0-pre.10 begin{% endcomment %}
              {% liquid
                assign href_cd60dd9b = href
                assign href = collection_url
                assign style_cd60dd9b = style
                assign style = 'secondary'
                assign size_cd60dd9b = size
                assign size = 'medium'
                assign text_cd60dd9b = text
                assign text = continue_shopping
                assign attributes_cd60dd9b = attributes
                assign attributes = 'tabindex="0"'
              %}
              {% comment %}
                @param href [Required]
                  This is the anchor tag for the link button.
              
                @param style [Required]
                  This is the style of the button: primary, secondary, tertiary, or text.
              
                @param size [Required]
                  This defines the size of the button: small, medium, or large.
              
                @param text [Required]
                  This is the text to be included in the button.
              
                @param attributes [Optional]
                  Any additional attributes to add to the button-link tag.
              {% endcomment %}
              
              <a
                data-component="button-link"
                data-component-type="component"
                {% if tag != blank %}data-component-tag="{{ tag }}"
                {% endif %}class="button-link button-link--{{ style }} button-link--{{ size }}{% if style == 'primary' or style == 'secondary' %} button-link--shadow{% endif %%}"
                {% if href != blank %}
                  href="{{ href }}"
                {% endif %}
                {{ attributes }}
              >
                {{ text }}
              </a>
              
              {% liquid
                assign href = href_cd60dd9b
                assign href_cd60dd9b = nil
                assign style = style_cd60dd9b
                assign style_cd60dd9b = nil
                assign size = size_cd60dd9b
                assign size_cd60dd9b = nil
                assign text = text_cd60dd9b
                assign text_cd60dd9b = nil
                assign attributes = attributes_cd60dd9b
                assign attributes_cd60dd9b = nil
              %}
