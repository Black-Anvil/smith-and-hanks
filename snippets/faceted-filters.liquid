          <a
            data-component="filters-tag-link"
            data-component-type="component"
            {% if tag != blank %}data-component-tag="{{ tag }}"
            {% endif %}class="filters-tag-link"
            href="{{ href }}"
            {{ attributes }}
          >
            {{ text }}
          </a>
          
          {% liquid
            assign href = href_f1a52b8d
            assign href_f1a52b8d = nil
            assign text = text_f1a52b8d
            assign text_f1a52b8d = nil
          %}
          {% comment %}Component filters-tag-link:default:1.0.0-pre.10 end{% endcomment %}
        {%- endfor -%}
        {% if filter.type == "price_range" %}
          {%- if filter.min_value.value != nil or filter.max_value.value != nil -%}
            {% capture button_text %}
              {% if filter.min_value.value %}{{ filter.min_value.value | money }}{% else %}{{ 0 | money }}{% endif %} - {% if filter.max_value.value %}{{ filter.max_value.value | money }}{% else %}{{ filter.range_max | money }}{% endif %}
            {% endcapture %}
            {% comment %}Component filters-tag-link:default:1.0.0-pre.10 begin{% endcomment %}
            {% liquid
              assign href_bbfc8f12 = href
              assign href = filter.url_to_remove
              assign text_bbfc8f12 = text
              assign text = button_text
            %}
            {% comment %}
              @param href [Required]
                This is the anchor tag for the tag button.
            
              @param text [Required]
                This is the text to be included in the button.
            
              @param attributes [Optional]
                Any additional attributes to add to the button-link tag.
            {% endcomment %}
            
            <a
              data-component="filters-tag-link"
              data-component-type="component"
              {% if tag != blank %}data-component-tag="{{ tag }}"
              {% endif %}class="filters-tag-link"
              href="{{ href }}"
              {{ attributes }}
            >
              {{ text }}
            </a>
            
            {% liquid
              assign href = href_bbfc8f12
              assign href_bbfc8f12 = nil
              assign text = text_bbfc8f12
              assign text_bbfc8f12 = nil
            %}
            {% comment %}Component filters-tag-link:default:1.0.0-pre.10 end{% endcomment %}
          {%- endif -%}
        {% endif %}
      {%- endfor -%}
      <a class="faceted-filters__clear-all" href="{{ filter_context.url }}?sort_by={{ filter_context.sort_by }}{% if filter_context == search and filter_context.terms %}&q={{ filter_context.terms }}{% endif %}" >
        {{ 'products.filters.clear_all' | t }}
      </a>
    </div>
  </form>
  {% endif %}
</div>