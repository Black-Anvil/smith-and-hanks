                      {{ heading }}
                    {% else %}
                      {{ current_name }}
                    {% endif %}
                  </button>
                  <div
                    class="disclosure__modal"
                    id="{{ namespace }}-list-{{ unique_id }}"
                    data-disclosure-modal
                  >
                    <h2 class="disclosure__heading">{{ heading }}</h2>
                    <button
                      class="disclosure__close"
                      data-disclosure-close
                    >
                      <span class="visually-hidden">{{ 'general.close' | t }}</span>
                    </button>
                    <ul class="disclosure__list">
                      {% for value_item in value_list %}
                        <li class="disclosure__list-item">
                          <a
                            class="
                              disclosure__link
                              {% if value_item[value_key] == current_value %}disclosure__link--selected{% endif %}
                            "
                            href="#"
                            data-value="{{ value_item[value_key] }}"
                            {% if value_item[value_key] == current_value %}
                              aria-current="true"
                            {% endif %}
                            data-disclosure-option
                          >
                            {% if is_country_selector %}
                              {{ value_item.name }} ({{ value_item.currency.iso_code }} {{ value_item.currency.symbol }})
                            {% else %}
                              {{ value_item[name_key] }}
                            {% endif %}
                          </a>
                        </li>
                      {% endfor %}
                    </ul>
                  </div>
                </div>
              </div>
              
              {% liquid
                assign namespace = namespace_1523448f
                assign namespace_1523448f = nil
                assign heading = heading_1523448f
                assign heading_1523448f = nil
                assign value_list = value_list_1523448f
                assign value_list_1523448f = nil
                assign value_key = value_key_1523448f
                assign value_key_1523448f = nil
                assign name_key = name_key_1523448f
                assign name_key_1523448f = nil
                assign current_value = current_value_1523448f
                assign current_value_1523448f = nil
                assign input_name = input_name_1523448f
                assign input_name_1523448f = nil
              %}
              {% comment %}Component disclosure:default:1.0.0-pre.10 end{% endcomment %}
            {%- endif -%}
          </div>
        {%- endif -%}

        {%- if section.settings.filters_show_totals -%}
          <div class="faceted-filters__result-count" role="status" data-filter-result-count>
            {%- if filter_context == search -%}
              {%- if search.results_count > 0 -%}
                {{ 'products.filters.product_count_simple' | t: count: search.results_count }}
              {%- endif -%}
            {%- else -%}
              {%- if section.settings.filters_show_filters -%}
                {%- if filter_context.products_count == filter_context.all_products_count -%}
                  {{ 'products.filters.product_count_simple' | t: count: filter_context.products_count }}
                {%- else -%}
                  {{ 'products.filters.product_count' | t: product_count: filter_context.products_count, count: filter_context.all_products_count }}
                {%- endif -%}
              {%- else -%}
                {% assign offset_max = paginate.current_offset | plus: paginate.page_size %}
                {% if offset_max > paginate.items %}
                  {% assign offset_max = paginate.items %}
                {% endif %}
                {% assign current_count = paginate.current_offset | plus: 1 | append: '-' | append: offset_max %}
                {{ 'products.filters.product_count' | t: product_count: current_count, count: paginate.items }}
              {%- endif -%}
            {%- endif -%}
          </div>
        {%- endif -%}
      </div>
    {% endif %}

    {% assign active_filter_count = 0 %}
    {%- for filter in filter_context.filters -%}
      {%- for value in filter.active_values -%}
        {% assign active_filter_count = active_filter_count | plus: 1 %}
      {%- endfor -%}
      {% if filter.type == "price_range" %}
        {%- if filter.min_value.value != nil or filter.max_value.value != nil -%}
          {% assign active_filter_count = active_filter_count | plus: 1 %}
        {% endif %}
      {% endif %}
    {%- endfor -%}

    <div
      class="
        faceted-filters__active
        {% if active_filter_count == 0 %}hidden{% endif %}
      "
      data-active-filters
      data-active-filters-count="{{ active_filter_count }}"
    >
      {%- for filter in filter_context.filters -%}
        {%- for value in filter.active_values -%}
          {% assign button_text = value.label | escape %}
          {% comment %}Component filters-tag-link:default:1.0.0-pre.10 begin{% endcomment %}
          {% liquid
            assign href_f1a52b8d = href
            assign href = value.url_to_remove
            assign text_f1a52b8d = text
            assign text = button_text
          %}
          {% comment %}
            @param href [Required]
              This is the anchor tag for the tag button.
          
            @param text [Required]
